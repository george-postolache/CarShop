<Window x:Class="CarShop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CarShop"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="400">
    <Grid>
        <Button x:Name="btnSellCar" Content="Sell Car" HorizontalAlignment="Left" Margin="153,75,0,0" VerticalAlignment="Top" Width="75" Click="btnSellCar_Click"/>
        <Button x:Name="btnCars" Content="Cars" HorizontalAlignment="Left" Margin="153,137,0,0" VerticalAlignment="Top" Width="75" Click="OpenCarsWindow"/>
        <Button x:Name="btnClients" Content="Clients" HorizontalAlignment="Left" Margin="153,195,0,0" VerticalAlignment="Top" Width="75" Click="btnClients_Click"/>

    </Grid>
</Window>
